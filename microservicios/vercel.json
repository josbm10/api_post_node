{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        },
        {
            "src":"./categorias/index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        },
        {
            "src":"/categorias(.*)",
            "dest":"/categorias/"
        }
    ]
}